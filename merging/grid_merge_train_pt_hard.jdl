# $1 = input file list
# $2 = output file name
# $3 = executable
# $4 = aliphysics version
# This is the startup script
Executable = "$3";
# Time after which the job is killed (500 min.)
TTL = "7200";
Output = {
   "AnalysisResults*.root"
};
Arguments = "$1 $2";
Packages = {
   "VO_ALICE@AliPhysics::$4",
   "VO_ALICE@APISCONFIG::V1.1x",
   "VO_ALICE@jemalloc::v3.6.0"
};
# JDL variables
JDLVariables = 
{
   "Packages"
};
